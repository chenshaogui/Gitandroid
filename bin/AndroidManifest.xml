<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.embestdkit"
    android:versionCode="1"
    android:versionName="1.0" >
    
    <uses-sdk
        android:minSdkVersion="8"
        android:targetSdkVersion="18" />

    
    <uses-permission android:name="android.permission.INTERNET" >
    </uses-permission>
    <uses-permission android:name="CALL_PHONE" >
    </uses-permission>
    <uses-permission android:name="android.permission.SEND_SMS" >
    </uses-permission>
    <uses-permission android:name="android.permission.ACCESS_WIFI_STATE" />

    <application
         android:allowBackup="true"
        android:icon="@drawable/icon"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        
        <uses-library android:name="android.test.runner" />
        
        <activity
            android:name="com.embestkit.test.DynamicLineChartActivity"
            android:label="@string/app_name"
            android:launchMode="singleTop"
            android:screenOrientation="landscape"
            android:theme="@style/AppTheme" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
            
        </activity>
        <activity android:name="com.embestdkit.zigbee.ZigBeeTool"></activity>
        <activity android:name="com.embestkit.login.RegisterUserActivity"></activity>
        <activity android:name="com.embestkit.login.MainFrameActivity"></activity>
        <activity android:name="com.embestkit.login.ForgetPasswordActivity"></activity>
        <activity android:name="com.embestkit.login.PersonalCenterActivity"></activity>
        <activity android:name="com.embestkit.login.MessageCenterActivity"></activity>
    </application>

   

</manifest>